  email = yuriy.toporovskyy@apocalypse333.com
  name = Yuriy Toporovskyy

[winUpdater]
  recentlySeenVersion = 2.16.1.windows.1

[core]
  editor = emacsclient -t -a=\\\"\\\"
  autocrlf = true
  pager = `test \"$TERM\" = \"dumb\" && echo cat || echo less`
  showbranch = true

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process

; [alias]
  ; ls-ignored = !git ls-files -v `git rev-parse --show-toplevel` | grep "^[[:lower:]]"
  ; ls-untracked = !echo "$@" && git status "$1" -s | grep '^??' | awk '{print $2}' && :
  ; pristine = !git reset $1 --hard && git clean -fX && git clean -fx && git clean -fd && :
  ; gc-all = !git -c gc.reflogExpire=0 -c gc.reflogExpireUnreachable=0 -c gc.rerereresolved=0 -c gc.rerereunresolved=0 -c gc.pruneExpire=now gc "$@"

[alias]
  ls-ignored = !"f(){ source ~/.gitaliases; git_ls_ignored $@; }; f"
  ls-untracked = !"f(){ source ~/.gitaliases; git_ls_untracked $@; }; f"
  pristine = !"f(){ source ~/.gitaliases; git_pristine $@; }; f"
  gc-all = !"f(){ source ~/.gitaliases; git_ls_gc_all $@; }; f"
  log-new = !"f(){ source ~/.gitaliases; git_log_new $@; }; f"
  ref2hash = !"f(){ source ~/.gitaliases; git_ref2hash $@; }; f"
  ls-status = !"f(){ source ~/.gitaliases; git_ls_status $@; }; f"
  add-to-commit = commit --amend --no-edit
  diffw = diff --color-words='[^[:space:]]|([[:alnum:]]|UTF_8_GUARD)+'

[lfs]
  skipdownloaderrors = true

[credential]
  helper = wincred

[color]
  ui = true
  branch = true
  diff = true
  interactive = true
  status = true
  grep = true
  pager = true
  decorate = true
  
[push]
  default = matching  